<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - multichain</title>
    <link rel="apple-touch-icon" sizes="180x180" href="~/img/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="~/img/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="~/img/favicon-16x16.png">
    <link rel="manifest" href="/site.webmanifest">
    <link rel="mask-icon" href="~/img/safari-pinned-tab.svg" color="#50c1e9">
    <meta name="apple-mobile-web-app-title" content="multichain">
    <meta name="application-name" content="multichain">
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body class="bg-neutral-100 dark:bg-neutral-800 text-gray-800 dark:text-gray-100 min-h-screen">
    @RenderBody()
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
